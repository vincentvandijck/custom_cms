{
  "name": "post-neon-cms",
  "version": "1.0.2",
  "private": false,
  "description": "custom cms for post-neon.com",
  "main": "electron.js",
  "scripts": {
    "start": "electron .",
    "opt": "node optimizeAll.js",
    "dev": "concurrently \"electron .\"",
    "build": "rimraf ./public && mkdir public && webpack --progress -p",
    "pack": "electron-packager . --overwrite post-neon-cms --icon=favicon.ico",
    "publish": "electron-builder --win -p always"
  },
  "publish": [
    {
      "provider": "github",
      "owner": "vincentvandijck",
      "repo": "https://github.com/vincentvandijck/custom-cms.git"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentvandijck/custom-cms.git"
  },
  "keywords": [
    "post",
    "neon",
    "custom",
    "cms"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "concurrently": "5.3.0",
    "css-loader": "^5.2.4",
    "electron": "^9.4.4",
    "electron-builder": "^22.10.5",
    "electron-winstaller": "^5.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.0",
    "rimraf": "3.0.2",
    "style-loader": "1.2.1",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "child_process": "^1.0.2",
    "classnames": "2.2.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "8.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-is-packaged": "^1.0.2",
    "electron-log": "^4.3.2",
    "electron-packager": "^15.2.0",
    "electron-updater": "^4.3.8",
    "express": "4.17.1",
    "ffmpeg-static": "^4.2.7",
    "ftp": "^0.3.10",
    "glob": "^7.1.6",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.19.1",
    "piping": "^0.3.2",
    "progress-stream": "^2.0.0",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.26.3",
    "ssh2-sftp-client": "^5.3.2",
    "uniqid": "^5.2.0",
    "update-electron-app": "^2.0.1",
    "url": "^0.11.0",
    "web-vitals": "^0.2.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 2 versions",
          "> 5%"
        ]
      },
      "postcss-nesting": {}
    }
  }
}
